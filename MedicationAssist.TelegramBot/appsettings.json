{
  "TelegramBot": {
    "Token": "",
    "WebhookUrl": null,
    "BotUsername": "MedicationAssistBot",
    "WebsiteUrl": "https://medications.meteoassist.space"
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "JwtSettings": {
    "Secret": "",
    "Issuer": "MedicationAssist",
    "Audience": "MedicationAssistUsers",
    "ExpirationInMinutes": 60
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Telegram.Bot": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/telegram-bot-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30
        }
      }
    ]
  }
}
